//Post project

curl -i -H "Accept: application/json" -H "Content-Type: application/json" -X POST -d '{ "projectVendor": "jira", "projectKey": "MANJEET","projectURL": "projectURL4", "authUser": "admin", "authKey": "Webhook@2016", "usersId" : ["manjeet", "ashutosh"] }' http://localhost:9000/v1/projects


//Post Issue
curl -i -H "Accept: application/json" -H "Content-Type: application/json" -X POST -d '{ "projectId": "284093190183", "vendorKey": "MANJ-26", "parentIssue": "123412","status": "DONE", "childIssue": "12321", "resolved": false}' http://localhost:9000/v1/issues
	
